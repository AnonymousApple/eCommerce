<!DOCTYPE html>
<html>
<head>
  <title>eCommerce</title>
  <%= csrf_meta_tags %>

  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  <link rel="stylesheet" href="https://code.jquery.com/ui/1.11.4/themes/smoothness/jquery-ui.css">
  <style type='text/css'>
  body {
    background-image: url("data:image/png;base64,");
    background-repeat: no-repeat;
    background-size: cover;
  }
  nav {
    background-color: black;
  }
  ul {
    list-style-type: none;
  }
  li {
    padding: 20px;
  }
  a {
    color: red;
    text-decoration: none;
  }
  a:link {
    color: teal;
  }
  a:hover {
    text-decoration: underline;
  }
  li {
    display: inline;
    margin: 5px;
  }
  nav {
    height: 100px;
  }
  img {
    width: 200px;
    height: 150px;
  }
  ol {
    list-style-type: upper-alpha;
  }
  .btn-primary {
    background-image: linear-gradient(navy, blue);
  }
  .btn-success {
    background-image: linear-gradient(lime, green);
  }
  .btn-warning {
    background-image: linear-gradient(orange, red);
  }
  .btn-danger {
    background-image: linear-gradient(crimson, red);
  }
  td {
    border: 5px groove #ffaadd;
  }
  .restock {
    background-image: linear-gradient(blue, aqua);
    padding: 5px;
    border-radius: 5px;
  }
  .btn {
    margin: 5px;
  }
  .pagination {
    background-color: transparent;
    padding: 0;
  }
  .restock:active {
    background-image: linear-gradient(aqua, blue);
  }
  .btn-success:active {
    background-image: linear-gradient(green, lime);
  }
  .btn-danger:active {
    background-image: linear-gradient(red, crimson);
  }
  .btn-warning:active {
    background-image: linear-gradient(red, orange);
  }
  .btn-primary:active {
    background-image: linear-gradient(blue, navy);
  }
  .pagination a:visited {
    color: black;
  }
  .btn-default {
    background-image: linear-gradient(black, gray);
  }
  .btn-default:active {
    background-image: linear-gradient(gray, black);
  }
  .pagination a:hover {
    background-color: transparent;
  }
  .table td {
    background-color: rgba(0, 0, 0, 0.5);
  }
  th {
    border: 5px groove red;
  }
  ol {
    list-style-type: decimal-leading-zero;
  }
  .danger-zone {
    border: 1px solid red;
    border-radius: 5px;
  }
  </style>
</head>

<body>
  <nav id='navbar' class='navbar-collapse collapse'>
    <ul>
      <li><%= link_to 'Make New Product', new_product_path, class: 'btn btn-default' %></li>
      <li><a href="/storefront/all_items">All Products</a></li>
      <li class="dropdown">
        <a href="#" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-haspopup="true" aria-expanded="false">Categories <span class="caret"></span></a>
        <ul class="dropdown-menu">
          <% @categories.each do |category| %>
          <li><%= link_to category.name, categorical_path(category_id: category.id) %></li>
          <% end %>
        </ul>
      </li>
      <li class="dropdown">
        <a href="#" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-haspopup="true" aria-expanded="false">Brands<span class="caret"></span></a>
        <ul class="dropdown-menu">
          <% @brands.each do |brand| %>
          <li><%= link_to brand, branding_path(brand: brand) %></li>
          <% end %>
          <li><%= link_to 'All', root_path %></li>
        </ul>
      </li>
      <ul class="nav navbar-nav navbar-right">
      <li><a href="/view_order">View Cart</a></li>
        <% if user_signed_in? %>
        <li class="dropdown">
          <a href="#" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-haspopup="true" aria-expanded="false">
            <%= image_tag current_user.avatar.url, style: 'border-radius: 5px; width: 35px; height: 35px;' if !current_user.avatar.blank? %> Hello, <%= current_user.username %>! <span class="caret"></span></a>
            <ul class="dropdown-menu">
              <li><%= link_to "Edit Profile", edit_user_registration_path %></li>
              <li><%= link_to "Sign Out", destroy_user_session_path, method: :delete %></li>
            </ul>
          </li>
          <% else %>
          <li><%= link_to "Sign In", new_user_session_path %></li>
          <% end %>
        </ul>
      </ul>
    </nav>
    <% if notice != nil %>
      <p id='notice' class='alert alert-info'><%= notice %></p>
    <% end %>
    <div class='container'>
      <%= yield %>
    </div>
    <script src="https://code.jquery.com/ui/1.11.4/jquery-ui.min.js"></script>
    <script>
      $(function() {
        $('#resizable').resizable({
          ghost: true
        });
      });
      $(function() {
        $('*').disableSelection();
      });
    </script>
  </body>
  </html>
